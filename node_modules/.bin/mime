#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rabi/code/quidkey/services/iframe/stripe-sample-code/node_modules/mime/node_modules:/Users/rabi/code/quidkey/services/iframe/stripe-sample-code/node_modules:/Users/rabi/code/quidkey/services/iframe/node_modules:/Users/rabi/code/quidkey/services/node_modules:/Users/rabi/code/quidkey/node_modules:/Users/rabi/code/node_modules:/Users/rabi/node_modules:/Users/node_modules:/node_modules:/Users/rabi/code/quidkey/services/iframe/stripe-sample-code/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/rabi/code/quidkey/services/iframe/stripe-sample-code/node_modules/mime/node_modules:/Users/rabi/code/quidkey/services/iframe/stripe-sample-code/node_modules:/Users/rabi/code/quidkey/services/iframe/node_modules:/Users/rabi/code/quidkey/services/node_modules:/Users/rabi/code/quidkey/node_modules:/Users/rabi/code/node_modules:/Users/rabi/node_modules:/Users/node_modules:/node_modules:/Users/rabi/code/quidkey/services/iframe/stripe-sample-code/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
